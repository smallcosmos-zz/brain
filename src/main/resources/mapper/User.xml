<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.tyut.wrx.brain.dao.UserMapper">
    <insert id="insertUser" parameterType="User">
        insert into
        t_user
        values(#{id},#{userName},#{userGender},#{userAge},#{userEduLevel},#{note})
    </insert>
</mapper>