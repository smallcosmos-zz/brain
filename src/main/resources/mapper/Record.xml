<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.tyut.wrx.brain.dao.RecordMapper">
    <insert id="addRecordToDb" parameterType="Record" useGeneratedKeys="true" keyProperty="id">
        insert into
        t_record(user_id,org_id,star_time,end_time,score,game_id)
        values(#{userId},#{orgId},#{startTime},#{endTime},#{score},#{gameId})
    </insert>
</mapper>